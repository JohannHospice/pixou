{
  "name": "functions",
  "version": "0.0.1",
  "author": "Johann Hospice",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"grapher build/plot.json\" \"npm run test\"",
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy",
    "deploy:ci": "firebase deploy -P $FIREBASE_PROJECT_ID --token $FIREBASE_TOKEN",
    "logs": "firebase functions:log",
    "test": "jest --maxWorkers=1 --setupFiles dotenv/config --watchAll"
  },
  "engines": {
    "node": "16"
  },
  "main": "src/index.js",
  "dependencies": {
    "@binance/connector": "^1.8.1",
    "axios": "^0.26.0",
    "dotenv": "^7.0.0",
    "firebase": "*",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "moment": "^2.29.1",
    "technicalindicators": "^3.1.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.17.3",
    "firebase-functions-test": "^0.3.3",
    "firebase-tools": "^10.2.2",
    "grapher": "*",
    "jest": "^27.5.1",
    "plotly": "^1.0.6",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
