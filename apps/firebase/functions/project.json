{
  "root": "apps/functions",
  "sourceRoot": "apps/functions/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/functions/cypress.json",
        "devServerTarget": "crinapp:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "crinapp:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/functions/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["crinapp"]
}
